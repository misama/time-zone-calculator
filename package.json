{
  "name": "time-zone-calculator",
  "version": "0.0.1",
  "scripts": {
    "build": "tsc",
    "prebuild": "rm -rf build && npm run build:fe",
    "build:fe": "cross-env NODE_ENV=use_local_server webpack",
    "dev": "npm run build:fe && nodemon --exec node --inspect -r ts-node/register ./src/App.ts",
    "dev:fe": "webpack serve",
    "start": "node ./build/App.js",
    "eslint:fix": "eslint --ignore-path .eslintignore --ext js,jsx,ts,tsx --fix ."
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.3",
    "express": "~4.16.1",
    "isomorphic-fetch": "^3.0.0",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/isomorphic-fetch": "0.0.36",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.1",
    "dotenv": "^16.0.1",
    "eslint": "^7.15.0",
    "html-webpack-plugin": "^4.3.0",
    "less-loader": "^6.2.0",
    "nodemon": "^2.0.6",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.12",
    "ts-node": "~10.7.0",
    "typescript": "~4.6.4",
    "webpack": "^5.10.1",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
